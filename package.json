{
    "name": "vue-phone-input-base",
    "version": "1.0.0",
    "description": "A Phone Input Component for Kazakhstan",
    "keywords": ["phone input", "input component", "kazakhstan"],
    "type": "module",
    "main": "dist/base-vue-phone-input.umd.cjs",
    "module": "dist/base-vue-phone-input.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/base-vue-phone-input.js",
            "types": "./dist/index.d.ts",
            "require": "./dist/base-vue-phone-input.umd.cjs"
        }
    },
    "scripts": {
        "build:types": "vue-tsc",
        "build": "npm run build:types && vite build",
        "prepublish": "npm run build"
    },
    "files": [
        "dist/"
    ],
    "devDependencies": {
        "@types/node": "^25.2.3",
        "@vitejs/plugin-vue": "^6.0.4",
        "typescript": "^5.9.3",
        "vite": "^7.3.1",
        "vite-plugin-dts": "^4.5.4",
        "vue-tsc": "^3.2.4"
    },
    "peerDependencies": {
        "vue": "^3.5.28"
    },
    "dependencies": {
        "libphonenumber-js": "^1.12.36"
    }
}
